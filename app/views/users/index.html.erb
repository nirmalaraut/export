

<h1><text-align :center;>Menu Item</h1>
<h1>
<%= link_to "Export CSV",  users_path(format: :csv), class: 'btn btn_primary' %>
</h1>
<div class= "media">
  <div media= "media_body">
    <table class="table table_hover">
            <thead>
              <div>
              <tr>

                <th>item&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
                <th>total_sales&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
                <th>percentage_of_gross &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>

                <th colspan="3"></th>
              </tr>
              </div>
            </thead>

            <tbody>
              <% @users.each do |user| %>
                <tr>
                  <td><%= user.first_name %></td>
                  <td><%= user.last_name %></td>
                  <td><%= user.Phone %></td>
                    <td><%= user.address %></td>
                  <td><%= link_to 'Show', user %></td>

                </tr>
                <% end %>
              </tbody>
      </table>
  </div>
</div>
<br>
<%= link_to 'New User', new_user_path %>
</br>
